    "f_cpu": "64000000L",
    "ldscript": "nrf52840_s140_v6.ld",
    "mcu": "nrf52840",
    "variant": "pca10059",
    "usb_product": "nRF52840 DK",	
    "hwids": [
      [
        "0x239A",
        "0x8029"
      ],
      [
        "0x239A",
        "0x003F"
      ],
      [
        "0x239A",
        "0x0040"
      ],
      [
        "0x239A",
        "0x8040"
      ]
